{
  "expo": {
    "name": "Bruktskatter",
    "slug": "Bruktskatter",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "bruktskatter",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "no.skattekammeratene.bruktskatter"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "no.skattekammeratene.bruktskatter"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@maplibre/maplibre-react-native"
    ],
    "extra": {
      "appScheme": "bruktskatter",
      "router": {},
      "eas": {
        "projectId": "54f1420a-5009-4575-8e01-f3ff52113284"
      }
    },
    "owner": "kjellmagne"
  }
}
